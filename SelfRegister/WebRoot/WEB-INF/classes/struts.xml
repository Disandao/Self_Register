<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default" namespace="/" extends="struts-default">
		<action name="Login" class="actions.Login_Action">
			<result name="error">index.jsp</result>
			<result name="patient">main.jsp</result>
			<result name="admin">admin.jsp</result>
			<result name="doctor">doctor.jsp</result>
		</action>
		
		<action name="Register" class="actions.Register_Action">
			<result name="SUCCESS">index.jsp</result>
			<result name="FAILED">re_fail.jsp</result>
		</action>
		<action name="Patientinf" class="actions.Patientinf_Action">
			<result name="SUCCESS">doctor_detail.jsp</result>
			
		</action>
		 <action name="DepartList" class="actions.DepartmentDetail_Action">
            <result name="depart_list">department_information.jsp</result>
        </action>
		<action name="DoctorList" class="actions.DepartToDoc_Action">
            <result name="departtodoc_list">departtodoc.jsp</result>
        </action>		
        <action name="DoctorDetail" class="actions.DoctorDetail_Action">
            <result name="doctor_detail">doctor_detail.jsp</result>
        </action>	
		
	</package>
</struts>    
